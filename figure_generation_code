    "ax2.bar(bins, np.insert(PSmonths,0,0), bottom = np.insert(S2LSmonths,0,0),color='purple',width=1,edgecolor='k')\n",
    "ax2.tick_params(labelsize=fs)\n",
    "ax2.set_xlabel('Month', fontsize=fs)\n",
