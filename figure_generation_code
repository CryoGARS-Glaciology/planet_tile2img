  {
   "cell_type": "code",
   "execution_count": 235,
   "id": "ce217498",
   "metadata": {},
   "outputs": [],
   "source": [
    "def season_count(df, ystart, mstart, yend, mend):\n",
    "    import pandas\n",
    "    import datetime\n",
    "    df = df[(df.datetime >= datetime.datetime(ystart,mstart,1)) & \n",
    "            (df.datetime < datetime.datetime(yend,mend,1))]\n",
    "    winter_count = 0\n",
    "    summer_count = 0\n",
    "    spring_count = 0\n",
    "    fall_count = 0\n",
    "    for idx, row in df.iterrows():\n",
    "        month = str(row.date)[4:6]\n",
    "        if month in ['01','02','12']:\n",
    "            winter_count += 1\n",
    "        if month in ['03','04','05']:\n",
    "            spring_count += 1\n",
    "        if month in ['06','07','08']:\n",
    "            summer_count += 1\n",
    "        if month in ['09','10','11']:\n",
    "            fall_count += 1\n",
    "    print('Winter:',winter_count)\n",
    "    print('Spring:',spring_count)\n",
    "    print('Summer:',summer_count)\n",
    "    print('Fall:',fall_count)\n",
    "    print('Total:',len(df))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 244,
   "id": "64bf86f0",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Winter: 14\n",
      "Spring: 10\n",
      "Summer: 13\n",
      "Fall: 13\n",
      "Total: 50\n"
     ]
    }
   ],
   "source": [
    "season_count(LS_df,2020,1,2022,1)"
   ]
  },
